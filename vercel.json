{
"functions": {
  "api/**/*.php": {
    "runtime": "vercel-php@latest"
  }
}
,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*)\\.php$",
      "dest": "/api/$1.php"
    },
    {
      "src": "/",
      "dest": "/api/dashboard.php"
    }
  ],
  "env": {
    "PHP_VERSION": "8.1"
  },
  "regions": ["gru1"]
}
